					<div class="card-img-overlay">
						<div class="card-body">
							<a href="{{path('app_portfolio_single',{"slug":item.getslug})}}" class="h4 text-white">{{item.getHeading}}</a>
							<ul class="meta-list list-unstyled d-flex">
								<li class="list-inline-item text-white fw-normal">{{item.getTechnology}}</li>
								<li class="list-inline-item text-white fw-normal">{{item.getCategory}}</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		{% endfor %}
		<div class="col-12 mx-auto text-center">
			<a href="{{path('app_portfolio')}}" class="btn btn-outline-primary text-capitailize fw-medium py-2">view all</a>
		</div>
	</div>
</div></div></section><!--================== Project Section End ==================-->{% endblock %}
